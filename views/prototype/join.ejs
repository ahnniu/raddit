<!-- Page: Sign up -->
<div class="container">
  <div class="row"></div>
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="panel-title">新用户注册</div>
      </div>
      <div class="panel-body">
        <form action="" class="join">
          <div class="form-group">
            <label for="userEmail">Email Address</label>
            <input type="email" class="form-control" id="userEmail" placeholder="Email" >
          </div>
          <div class="form-group">
            <label for="userPwd">Password</label>
            <input type="password" class="form-control" id="userPwd" placeholder="Password" >
          </div>
          <div class="form-group">
            <label for="userConfirmPwd">Confirm your password</label>
            <input type="password" class="form-control" id="userConfirmPwd" placeholder="Password again">
          </div>
          <button type="submit" class="btn btn-default">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>